<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.alcaj.barbroslokaltrafik.DialogImageArtOnWay">

    <FrameLayout
        android:id="@+id/image_dialog_root"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/fadedBlack"
        android:gravity="center">

        <ImageView
            android:id="@+id/imageViewDialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/artandetagfotfastethumbnail" />
    </FrameLayout>
</android.support.constraint.ConstraintLayout>
